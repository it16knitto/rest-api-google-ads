{
	"name": "rest-boilerplate-ts",
	"version": "0.0.1",
	"main": "index.ts",
	"scripts": {
		"build": "rm -fr dist/* && npx tsc",
		"start": "node dist/index.js",
		"dev": "tsx --watch src/index.ts",
		"migrate": "npx knex --knexfile=./database/knexfile.js migrate:latest",
		"migrate:up": "npx knex --knexfile=./database/knexfile.js migrate:up",
		"migrate:down": "npx knex --knexfile=./database/knexfile.js migrate:down",
		"migrate:make": "npx knex --knexfile=./database/knexfile.js migrate:make",
		"lint": "eslint --fix **/*.ts"
	},
	"keywords": [],
	"author": "Knitto Textile Dept IT",
	"license": "MIT",
	"dependencies": {
		"@knittotextile/knitto-core-backend": "^0.5.1",
		"amqplib": "^0.10.3",
		"bcryptjs": "^2.4.3",
		"class-sanitizer": "^1.0.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"cors": "^2.8.5",
		"dotenv": "^16.1.3",
		"jsonwebtoken": "^9.0.0",
		"mysql2": "^3.6.5",
		"tslib": "^2.6.2",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@types/amqplib": "^0.10.4",
		"@types/bcryptjs": "^2.4.2",
		"@types/cors": "^2.8.13",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/node": "^20.5.9",
		"@types/uuid": "^9.0.7",
		"@typescript-eslint/eslint-plugin": "^6.4.0",
		"eslint": "^8.55.0",
		"eslint-config-standard-with-typescript": "^42.0.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
		"eslint-plugin-promise": "^6.0.0",
		"husky": "^8.0.3",
		"knex": "^3.1.0",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3"
	}
}
